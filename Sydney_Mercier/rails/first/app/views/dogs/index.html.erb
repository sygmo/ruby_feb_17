<h1>Dog#index</h1>
<p>Find me in app/views/dogs/index.html.erb</p>

<% for dog in @dogs %>
	Id: <%= dog.id %><br>
	Name: <%= dog.name %><br>
	Breed: <%= dog.breed %><br>
	<a href="/dogs/<%=dog.id%>">Show</a>
	<a href="/dogs/<%=dog.id%>/edit">Edit</a>
	<form method="POST" action="/dogs/<%= dog.id %>">
		<input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>" />
		<input type="hidden" name="_method" value="DELETE">
		<input type="submit">
	</form>
	<hr>
<% end %>
